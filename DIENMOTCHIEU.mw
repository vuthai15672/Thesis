             if Unify_Fact(il,j)
                 then temp:=subs(j=il,temp); fi;
          od;
       od; 
       if temp &lt;&gt; fact2 and 
          Unify_Fact(fact1,temp)
             then return true;
       fi;        
    fi;
  fi; 
  return false ;</Font>
<Font foreground="[0,0,255]">end : # Unify_Fact</Font></Text-field>
